{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "react/jsx-filename-extension": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "root": ["./client/js"]
      }
    }
  },
  "env": {
    "jest": true
  }
}
