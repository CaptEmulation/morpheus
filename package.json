{
  "name": "webgl",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build:server": "babel server -d lib --copy-files",
    "build:client": "gulp build:client",
    "build:client:watch": "gulp build:client:watch",
    "gulp": "gulp",
    "db:update": "gulp db:update | bunyan",
    "webpack": "webpack",
    "db:prime": "gulp db:prime | bunyan",
    "start": "npm run build:client & npm run build:server && node lib/index",
    "start:dev": "gulp serve:dev | bunyan",
    "test:client": "gulp test:client",
    "test:client:watch": "gulp test:client:watch"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client/js/**/__tests__/**/*.js?(x)",
      "<rootDir>/client/js/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
